    "name" : "makinacorpus/goat-domain",
    "type": "library",
    "description": "Legacy code for implementing entity repositories, alive only for legacy projects maintainance purpose.",
    "keywords": ["orm", "postgresql", "mysql", "database", "symfony", "pdo"],
    "homepage": "http://github.com/makinacorpus/php-goat-domain",
    "license": "GPL-2.0-or-later",
